<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="500.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="application.ETransferController">
   <children>
      <Button fx:id="backButton" layoutX="300.0" layoutY="300.0" mnemonicParsing="false" onAction="#backClicked" text="Back" />
      <Button fx:id="etransferButton" layoutX="300.0" layoutY="250.0" mnemonicParsing="false" onAction="#etransferClicked" text="Confirm E-Transfer" />
      <Label fx:id="errorLabel" layoutX="225.0" layoutY="20.0" prefHeight="35.0" prefWidth="365.0" />
      <Label fx:id="accountLabel" layoutX="300.0" layoutY="50.0" text="Account to transfer from: " />
      <Label fx:id="amountLabel" layoutX="300.0" layoutY="120.0" text="Amount to Transfer: " />
      <Label fx:id="userLabel" layoutX="300.0" layoutY="190.0" text="ID of user recieving money: " />
      <TextField fx:id="accountText" layoutX="300.0" layoutY="70.0" />
      <TextField fx:id="amountText" layoutX="300.0" layoutY="140.0" />
      <TextField fx:id="userText" layoutX="300.0" layoutY="210.0" />
      <Label fx:id="summaryLabel" alignment="TOP_LEFT" layoutX="5.0" layoutY="5.0" prefHeight="395.0" prefWidth="250.0" wrapText="true" />
   </children>
</AnchorPane>
