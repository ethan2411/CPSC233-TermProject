<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.NewSavingsController">
   <children>
      <Label fx:id="summaryLabel" alignment="TOP_LEFT" layoutX="5.0" layoutY="5.0" prefHeight="395.0" prefWidth="250.0" />
      <Label fx:id="nameLabel" layoutX="300.0" layoutY="120.0" text="Name of Savings Account: " />
      <TextField fx:id="nameText" layoutX="300.0" layoutY="140.0" />
      <Button fx:id="createButton" layoutX="300.0" layoutY="220.0" mnemonicParsing="false" onAction="#createCkicked" text="Confirm Account Creation" />
      <Button fx:id="backButton" layoutX="300.0" layoutY="300.0" mnemonicParsing="false" onAction="#backClicked" text="Back" />
      <Label fx:id="errorLabel" layoutX="225.0" layoutY="20.0" prefHeight="35.0" prefWidth="365.0" />
   </children>
</AnchorPane>
