<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="650.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainMenuController">
   <children>
      <Label fx:id="accountInfoLabel" layoutX="14.0" layoutY="14.0" prefHeight="35.0" prefWidth="182.0" text="Account Info:" />
      <Label fx:id="accountsSummaryLabel" alignment="TOP_LEFT" layoutX="14.0" layoutY="59.0" prefHeight="583.0" prefWidth="386.0" wrapText="true" />
      <Label layoutX="478.0" layoutY="14.0" text="What would you like to do?" />
      <Button fx:id="depositButton" layoutX="488.0" layoutY="107.0" mnemonicParsing="false" onAction="#depositClicked" text="Deposit" />
      <Button fx:id="withdrawButton" layoutX="478.0" layoutY="182.0" mnemonicParsing="false" onAction="#withdrawClicked" text="Withdraw" />
      <Button fx:id="transferButton" layoutX="488.0" layoutY="261.0" mnemonicParsing="false" onAction="#transferCLicked" text="Transfer" />
      <Button fx:id="newAccountButton" layoutX="488.0" layoutY="351.0" mnemonicParsing="false" onAction="#newAccountClicked" text="Open A New Account" />
      <Button fx:id="logoutButton" layoutX="876.0" layoutY="585.0" mnemonicParsing="false" onAction="#logoutButtonClicked" text="Logout" />
      <Label fx:id="userIDLabel" layoutX="838.0" layoutY="14.0" />
   </children>
</AnchorPane>
